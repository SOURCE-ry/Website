{
  "locales": ["fi", "en"],
  "catalogs": [
    {
      "path": "<rootDir>/src/locale/{locale}/messages",
      "include": ["<rootDir>/src"],
      "exclude": ["**/node_modules/**", "**/__tests__/**"]
    }
  ],
  "format": "po",
  "extractBabelOptions": {
    "presets": [
      "@babel/preset-typescript",
      "babel-preset-gatsby"
    ]
  }
}
